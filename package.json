{
  "name": "elek.io",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "c8 turbo run test"
  },
  "devDependencies": {
    "c8": "^7.12.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.7.1",
    "turbo": "^1.4.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.10.0"
}
