{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "./packages/core/dist/**/*",
        "./packages/ui/dist/**/*",
        "./apps/client/out/**/*",
        "./apps/client/static/**/*"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["core#build", "ui#build"]
    },
    "test": {
      "outputs": [],
      "dependsOn": ["ui#build"]
    },
    "clean": {
      "cache": false
    }
  }
}
