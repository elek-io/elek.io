{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["core#build", "ui#build"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["ui#build"]
    },
    "clean": {
      "cache": false
    }
  }
}
